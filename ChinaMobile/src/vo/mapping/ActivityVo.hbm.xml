<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="vo.ActivityVo" table="Activity">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">test_id</param>
			</generator>
		</id>
		<property name="name" type="java.lang.String"></property>
		<property name="detail" type="java.lang.String"></property>
		<property name="city" type="java.lang.String"></property>
		<property name="start_time" type="java.lang.String"></property>
		<property name="end_time" type="java.lang.String"></property>
		<property name="image" type="java.lang.String"></property>
		<property name="state" type="java.lang.String"></property>
	</class>
</hibernate-mapping>